
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model University {
  id              String   @id @default(uuid())
  universityName            String
  country         String
  location        String   
  tuitionFee      Int      
  ranking         Int     
  establishedYear Int

  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
